{
  "name": "kings-league",
  "version": "1.0.1",
  "description": "Kings League Infojobs Project with API & Web",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "dev:api": "wrangler dev api/index.js",
    "dev": "astro dev",
    "preview": "astro preview",
    "publish:api": "wrangler publish api/index.js",
    "scrape": "node --no-warnings scraping/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "start": "astro dev",
    "lint": "eslint . --ext .js,.astro,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.astro,.ts,.tsx --fix",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@astrojs/prefetch": "0.1.1",
    "@astrojs/tailwind": "2.1.3",
    "astro": "1.9.1",
    "cheerio": "1.0.0-rc.12",
    "hono": "2.7.1",
    "husky": "8.0.3",
    "picocolors": "1.0.0",
    "tailwindcss": "3.2.4"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "5.48.0",
    "@vitest/coverage-c8": "0.26.3",
    "@vitest/ui": "0.26.3",
    "eslint": "8.31.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-astro": "0.21.1",
    "prettier": "2.8.1",
    "prettier-plugin-astro": "0.7.2",
    "vite": "4.0.4",
    "vitest": "0.26.3",
    "wrangler": "2.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint:fix",
      "pre-push": "npm test"
    }
  }
}
